generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Visitor {
  id        String   @id @default(cuid())
  name      String
  email     String   @unique
  phone     String
  college   String
  createdAt DateTime @default(now())
  verified  Boolean  @default(false)
}
